    "glob": "7.1.7",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "rollup": "2.53.1",
    "ts-node": "10.1.0",
    "ts-transformer-keys": "0.4.3",
